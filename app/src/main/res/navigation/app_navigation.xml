<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_navigation"
    app:startDestination="@id/dashboardFragment2">
    <fragment
        android:id="@+id/dashboardFragment2"
        android:name="id.ac.ukdw.pointofsale.ui.dashboard.DashboardFragment"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard" >
        <action
            android:id="@+id/action_dashboardFragment2_to_editMenuFragment2"
            app:destination="@id/editMenuFragment2" />
        <action
            android:id="@+id/action_dashboardFragment2_to_penjualanFragment"
            app:destination="@id/penjualanFragment" />
    </fragment>
    <fragment
        android:id="@+id/editMenuFragment2"
        android:name="id.ac.ukdw.pointofsale.ui.menu.EditMenuFragment"
        android:label="fragment_edit_menu"
        tools:layout="@layout/fragment_edit_menu" />
    <fragment
        android:id="@+id/penjualanFragment"
        android:name="id.ac.ukdw.pointofsale.ui.penjualan.PenjualanFragment"
        android:label="fragment_penjualan"
        tools:layout="@layout/fragment_penjualan" />
</navigation>